procedure TreSpecAll.DoRun(var mes :TMessage);
begin
    if(cbClass.KeyValue=Null) then
    begin
      DabData.ShowPopupMessage('      ');
      exit;
    end;
// if ((dblUnit.text<>'') and (dblCostCenter.text<>'') and (dblLocCode.text<>'') ) then
   begin
     i := 0;
     Case (cbClass.KeyValue) of
       1:
       begin
          ActiveQuery:=spSpecantique;
          ppViewer1.Report:=ppReport1;
       end;
       2:
       begin
          ActiveQuery:=spSpecMuseum;
          ppViewer1.Report:=ppReport2;
       end;
       3:
       begin
          ActiveQuery:=spSpecStamp;
          ppViewer1.Report:=ppReport3;
       end;
       4:
       begin
          ActiveQuery:=spSpecRide;
          ppViewer1.Report:=ppReport4;
       end;
       5:
       begin
          ActiveQuery:=spSpecLab;
          ppViewer1.Report:=ppReport5;
       end;
       6:
       begin
          ActiveQuery:=spSpecMachinery;
          ppViewer1.Report:=ppReport6;
       end;
       7:
       begin
          ActiveQuery:=spSpecBooks;
          ppViewer1.Report:=ppReport7;
       end;
       8:
       begin
          ActiveQuery:=spSpecLens;
          ppViewer1.Report:=ppReport8;
       end;
       9:
       begin
          ActiveQuery:=spSpecTextile;
          ppViewer1.Report:=ppReport9;
       end;
       10:
       begin
          ActiveQuery:=spSpecDiskette;
          ppViewer1.Report:=ppReport10;
       end;
       11:
       begin
          ActiveQuery:=spSpecTaxidermy;
          ppViewer1.Report:=ppReport11;
       end;
       12:
       begin
          ActiveQuery:=spSpecAnimal;
          ppViewer1.Report:=ppReport12;
       end;
       13:
       begin
          ActiveQuery:=spSpecIncon;
          ppViewer1.Report:=ppReport13;
       end;
       else
       begin
         DabData.ShowPopupMessage('      ');
         exit;
       end;
     end;
     ActiveQuery.close;
     if trim(dblunit.text)<>'' then
           ActiveQuery.Parameters[1].value := dblUnit.KeyValue
     else ActiveQuery.Parameters[1].value :=null;
     if trim(dblCostCenter.text)<>'' then
       ActiveQuery.Parameters[2].value := dblCostCenter.KeyValue
     else ActiveQuery.Parameters[2].value :=null;
     if trim(dblLocCode.text)<>'' then
        ActiveQuery.Parameters[3].value := dblLocCode.KeyValue
     else ActiveQuery.Parameters[3].value :=null;
     if trim(dblemp.text)<>'' then
        ActiveQuery.Parameters[4].value := dblemp.KeyValue
     else ActiveQuery.Parameters[4].value :=null;

     ActiveQuery.open;

     ppViewer1.reset;
     ppViewer1.Report.PrintToDevices;
  end;
   end;


