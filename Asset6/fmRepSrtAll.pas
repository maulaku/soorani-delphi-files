       end;
     end;//case
     ActiveQuery.close;
     ActiveQuery.Parameters[1].Value:=meFrom.Text;
     ActiveQuery.Parameters[2].Value:=meTo.Text;
     if edLabelFrom.Text<>'' then
        ActiveQuery.Parameters[3].Value:=edLabelFrom.Text
