  object QuRel: TADOQuery
    Connection = DabData.ADOC
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'p1'
        DataType = ftString
        Size = 1
        Value = Null
      end>
    SQL.Strings = (
      
        'select subsystems.syscode from (select distinct sysproperties.ty' +
        'pe from(select distinct sysobjects.id from( select * from sysref' +
        'erences left outer join sysobjects on  sysreferences.rkeyid=syso' +
        'bjects.id where [name] =:p1) a  join sysobjects on a.fkeyid=syso' +
        'bjects.id) b  join sysproperties on b.id=sysproperties.id) c  jo' +
        'in subsystems on c.type=subsystems.row'
      '')
    Left = 56
    Top = 32
    object QuRelsyscode: TSmallintField
      FieldName = 'syscode'
    end
  end
